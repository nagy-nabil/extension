{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background":{
        "service_worker": "./src/js/background.js",
        "type":"module"
    },
    "action": {
        "default_popup": "./src/popup.html"
    },
    "permissions": [ "storage","activeTab", "scripting"],
    "host_permissions": ["<all_urls>"]
}